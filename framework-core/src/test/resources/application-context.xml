<?xml version="1.0" encoding="UTF-8"?>
<beans>
    <bean id="userDao"
          class="cn.element.ioc.test.bean.UserDao"
          init-method="initDataMethod"
          destroy-method="destroyDataMethod"/>

    <bean id="userService" class="cn.element.ioc.test.bean.UserService">
        <property name="uid" value="10001"/>
        <property name="company" value="腾讯"/>
        <property name="location" value="深圳"/>
        <property name="userDao" ref="userDao"/>
    </bean>

    <bean class="cn.element.ioc.test.common.IBeanPostProcessor"/>
    <bean class="cn.element.ioc.test.common.IBeanFactoryPostProcessor"/>
</beans>
